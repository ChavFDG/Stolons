@model Stolons.ViewModels.Stolons.StolonViewModel

<form name="stolonForm" asp-action="EditStolon" asp-controller="Stolons" asp-route-id="@Model.Stolon.Id"  enctype="multipart/form-data">
    <div class="modal-body">
        @Html.Partial("_PartialEditStolonForm")
    </div>
    <div class="modal-footer">
        <input type="submit" value="Enregistrer les modifications" class="btn btn-small btn-default" />
    </div>
</form>

@section Scripts {
    <script>tinymce.init({ selector: 'textarea', language: 'fr_FR', menubar: false });</script>
}
