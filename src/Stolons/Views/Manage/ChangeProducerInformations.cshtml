@model Stolons.ViewModels.Producers.AdherentViewModel

@{
    ViewData["Title"] = "Modification de mes paramètres personnels";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Edit" enctype="multipart/form-data">
  @Html.Partial("Templates/EditAdherent");
  <div class="form-group">
    <div class="col-md-offset-2 col-md-10">
      <input type="submit" value="Sauvegarder  les modifications" class="btn btn-small btn-default" />
    </div>
  </div>
</form>

@section Scripts {
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
