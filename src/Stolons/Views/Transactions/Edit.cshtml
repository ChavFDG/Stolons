@model Stolons.Models.Transaction

<h2>Edition de la transaction du @Model.Date</h2>

<form asp-action="Edit">
    <div class="form-horizontal">
        @Html.Partial("Templates/EditTransaction")      
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Sauvegarder les modifications" class="btn btn-small btn-default" />
            </div>
        </div>
    </div>
</form>

<div>
    <a asp-action="Index" class="btn btn-small btn-default">Retourner aux sympathisants</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
