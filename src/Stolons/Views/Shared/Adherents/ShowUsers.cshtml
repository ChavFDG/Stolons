@model IEnumerable<Stolons.Models.Users.IAdherent>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(item => item.Name)
        </th>
        <th>
            @Html.DisplayNameFor(item => item.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(item => item.PostCode)
        </th>
        <th>
            @Html.DisplayNameFor(item => item.Email)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Surname)
            </td>
            <th>
                @Html.DisplayFor(modelItem => item.PostCode)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.Email)
            </th>
            <th></th>
        </tr>
    }
</table>

